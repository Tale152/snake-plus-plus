<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="root" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="layoutBox" alignment="CENTER" fillWidth="false" prefHeight="200.0" prefWidth="400.0" spacing="32.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="titleLabel" alignment="CENTER" contentDisplay="CENTER" text="You won/lost! (how?)" textAlignment="CENTER" />
            <Label fx:id="reasonLabel" text="You did/didn't the thing! (Again, how?)" />
            <Label fx:id="nextLevelNameLabel" text="Next level: name" textAlignment="CENTER" />
            <Label fx:id="nextLevelDescriptionLabel" text="Conditions:" textAlignment="CENTER" />
            <HBox>
               <children>
                  <Button fx:id="nextLevelButton" onAction="#nextLevel" text="Next Level" />
                  <Button fx:id="mainMenuButton" onAction="#goToMainMenu" text="Main menu" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
